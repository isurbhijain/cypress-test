{
  "name": "cypressprojectdemo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress open",
    "jenkinsTest": "npx cypress run --spec \"cypress/e2e/DemoWebShop.cy.js\"",
    "BDD": "npx cypress run --spec \"cypress/e2e/BDD/demo.feature\"",
    "Report": "npx cypress run --reporter cypress-mochawesome-reporter --spec \"cypress/e2e/DemoWebShop.cy.js\""
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true,
      "output": "cypress/CucmberReports/report.json"
    },
    "html": {
      "enabled": true,
      "output": "cypress/CucmberReports/report.html"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.1.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "convert-excel-to-json": "^1.7.0",
    "mocha": "^10.6.0",
    "multiple-cucumber-html-reporter": "^3.6.2"
  },
  "devDependencies": {
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "exceljs": "^4.4.0",
    "mochawesome": "^7.1.3",
    "neat-csv": "^5.2.0"
  }
}
